    "discord-bot"
  ],
  "author": "ashnel3",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/ashnel3/eboi/issues"
  },
  "homepage": "https://github.com/ashnel3/eboi#readme",
  "engines": {
    "node": ">= 18"
  },
  "engineStrict": true,
  "packageManager": "pnpm@9.5.0",
  "types": "./src/types",
  "dependencies": {
    "@prisma/client": "^5.16.2",
    "@supercharge/promise-pool": "^3.2.0",
    "discord.js": "^14.15.3",
    "i18next": "^23.12.1",
    "i18next-fs-backend": "^2.3.1",
    "i18next-resources-to-backend": "^1.2.1",
    "winston": "^3.13.1"
