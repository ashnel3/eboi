    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/js": "^9.7.0",
    "@types/node": "^20.14.10",
    "conventional-changelog-cli": "^5.0.0",
    "dotenv": "^16.4.5",
    "eslint": "^9.7.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "globals": "^15.8.0",
    "gulp": "^5.0.0",
    "gulp-if": "^3.0.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-typescript": "6.0.0-alpha.1",
    "husky": "^9.0.11",
    "prettier": "^3.3.2",
    "prisma": "^5.16.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.16.0",
    "vitest": "^2.0.2"
