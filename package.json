  "description": "Eboi Discord Ebay integration bot",
  "main": "dist/bin/index.js",
  "type": "module",
  "scripts": {
    "start": "gulp start",
    "dev": "gulp dev",
    "build": "gulp build",
    "clean": "gulp clean",
    "format": "pnpm run /^format:.*/",
    "format:prettier": "prettier -c -w .",
    "format:prisma": "prisma format",
    "lint": "pnpm run /^lint:.*/",
    "lint:es": "eslint .",
    "lint:tsc": "tsc --noEmit",
    "studio": "prisma studio",
